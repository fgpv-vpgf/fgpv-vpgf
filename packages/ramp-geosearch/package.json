{
    "name": "ramp-geosearch",
    "version": "3.0.1",
    "description": "Configurable geolocation search library",
    "main": "dist/index.js",
    "repository": "git@github.com:RAMP-PCAR/geosearch.git",
    "author": "Miles Petrov",
    "license": "MIT",
    "devDependencies": {
        "@types/jasmine": "2.8.8",
        "@types/jest": "23.1.2",
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
        "docute-cli": "0.6.0",
        "eslint": "^6.7.2",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-no-null": "^1.0.2",
        "jasmine": "3.1.0",
        "jasmine-ts": "0.2.1",
        "jest": "29.3.1",
        "node-fetch": "2.1.2",
        "nodemon": "1.17.5",
        "ts-jest": "29.0.3",
        "ts-loader": "4.4.1",
        "tslint": "5.18.0",
        "typescript": "3.6.2",
        "webpack": "4.39.1",
        "webpack-cli": "3.3.6"
    },
    "typings": "dist/index.d.ts",
    "scripts": {
        "build": "webpack --mode=production",
        "serve": "webpack --watch --mode=development",
        "test": "jest",
        "docute": "docute",
        "fetchdata": "node scripts/typeCompiler.js && node scripts/provinceCompiler.js"
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
